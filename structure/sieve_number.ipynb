   "source": [
    "def sieve(maxn):\n",
    "    isprime = [True for i in range(maxn+1)]\n",
    "    for k in range(2, maxn+1):\n",
    "        if isprime[k]:\n",
    "            yield k\n",
    "            for j in range(k*2, maxn+1, k):\n",
    "                isprime[j] = False\n",
    "\n"
   ]
